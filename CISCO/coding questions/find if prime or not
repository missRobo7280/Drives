// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

bool isPrime(int n)
{
    for(int i=2;i<sqrt(n);i++)
    {
        if(n%i==0)
        return 0;
    }
    return 1;
}
int main() {
    
    int n;cin>>n;
   if (isPrime(n))
    cout<<"Prime";
    else
    cout<<"NOT PRIME";
    return 0;
}
